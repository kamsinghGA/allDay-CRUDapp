<%- include('../partials/header') %>

<h1>All Flights</h1>

<table>
  <tr>
    <th>Flight Number</th>
  </tr>
  <% flights.forEach(function(flight) { %>
    <tr>
      <td><a href="/flights/<%= flight._id %>"><%= flight.flightNo %></a></td>
    </tr>
  <% }); %>
</table>

<%- include('../partials/footer') %>



